{
  "name": "mao-admin-panel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:preview": "cd frontend && npm run preview",
    "frontend:lint": "cd frontend && npm run lint",
    "frontend:lint:fix": "cd frontend && npm run lint:fix",
    "frontend:type-check": "cd frontend && npm run type-check",
    "frontend:clean": "cd frontend && npm run clean",
    "frontend:start": "cd frontend && npm run start",
    "backend:dev": "cd backend && npm run dev",
    "backend:install": "cd backend && npm install",
    "frontend:install": "cd frontend && npm install",
    "install:all": "npm install && npm run backend:install && npm run frontend:install",
    "dev:all": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "dev": "npm run dev:all"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "typescript": "^5.3.3"
  }
}
