{
  "textFontSize": 13,
  "interfaceFontSize": 13,
  "monospaceFontSize": 13,
  "lineHeight": 1.5,
  "showLineNumber": true,
  "showIndentGuide": true,
  "borderColorPrimary": "#3a3a3a",
  "borderColorSecondary": "#3a3a3a",
  "colorScheme": "dark",
  "showInlineTitle": true,
  "legacyEditor": false,
  "livePreview": true,
  "promptDelete": false,
  "alwaysUpdateLinks": true,
  "spellcheck": true,
  "spellcheckLanguages": [
    "en",
    "ru"
  ],
  "useMarkdownLinks": false,
  "showUnsupportedFiles": true,
  "newFileLocation": "folder",
  "newFileFolderPath": "Notes",
  "attachmentFolderPath": "Assets",
  "userIgnoreFilters": [
    "node_modules/",
    ".git/",
    ".vscode/"
  ],
  "strictLineBreaks": false,
  "showFrontmatter": true,
  "foldHeading": true,
  "foldIndent": true,
  "readableLineLength": true,
  "rightToLeft": false,
  "defaultViewMode": "source",
  "smartIndentList": true,
  "pdfExportSettings": {
    "includeName": false,
    "pageSize": "Legal",
    "landscape": false,
    "margin": "0",
    "downscalePercent": 100
  },
  "useTab": false,
  "tabSize": 2,
  "lineWrap": true,
  "autoConvertHtml": true,
  "autoPairBrackets": true,
  "autoPairMarkdown": true,
  "trashOption": "local",
  "newLinkFormat": "relative",
  "useDataviewInline": true,
  "focusNewTab": true,
  "enabledPlugins": [
    "calendar",
    "dataview",
    "obsidian-git",
    "templater-obsidian"
  ],
  "vimMode": false,
  "communityPluginSortOrder": "download",
  "communityThemeSortOrder": "download"
}